local TargetAimSection = CombatTab:Section({Name = bTyySMQWOaiIcGimkMelaCNiCPWSanoyIMYrZPxuRJyeCPswaGztshzKKnVKbXoSHZnpQbJLfCeeRfNvNaezpa('eitNStsRvCgnfQlfrOtgeaDXSJfXyPkDIwtbwPUlewYPVuSKgpOBIdnVGFyZ2V0IEFpbQ=='), Side = 1})

