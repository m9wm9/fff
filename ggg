--=====================================================================
-- NAME ESP
--=====================================================================

local ESPEnabled = false
local ESPColor = Color3.fromRGB(255, 255, 255)
local ESPCache = {}
local ESPConnection = nil

local function renderNameESP(plr)
    if not plr or ESPCache[plr] then return end
    local text = Drawing.new(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('fHksYWwVmxzPkEGnlYlUMdJaaPmzHldzZiMzKSNUTonvXYGABiOjxGnVGV4dA=='))
    text.Size = 14
    text.Font = 2
    text.Outline = true
    text.Center = true
    text.Visible = false
    ESPCache[plr] = text
end

local function removeNameESP(plr)
    if ESPCache[plr] then
        ESPCache[plr]:Remove()
        ESPCache[plr] = nil
    end
end

local function updateNameESP()
    if not ESPEnabled then return end
    local cam = CurrentCamera
    local localChar = LocalPlayer.Character
    if not localChar or not localChar:FindFirstChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('XvlpfadaBbkQtHvbsCUpaoDKciUExXfuMjOucRLbRJxJPWwhycgfzFxSHVtYW5vaWRSb290UGFydA==')) then return end

    for plr, text in pairs(ESPCache) do
        if not plr.Parent or not plr.Character or not plr.Character:FindFirstChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('zSTHcFvFZlSvtPtzbdPXiaFhQeDJSDsgSrkTpBuIoLfYAMjLKxpMdOlSHVtYW5vaWRSb290UGFydA==')) then
            text.Visible = false
            continue
        end

        local root = plr.Character.HumanoidRootPart
        local headPos = root.Position + Vector3.new(0, 3, 0)
        local screenPos, onScreen = cam:WorldToViewportPoint(headPos)

        if onScreen then
            text.Position = Vector2.new(screenPos.X, screenPos.Y)
            text.Text = plr.Name
            text.Color = ESPColor
            text.Visible = true
        else
            text.Visible = false
        end
    end
end

for _, plr in ipairs(Players:GetPlayers()) do
    if plr ~= LocalPlayer then
        renderNameESP(plr)
    end
end

Players.PlayerAdded:Connect(function(plr)
    if plr ~= LocalPlayer then
        plr.CharacterAdded:Connect(function()
            task.wait(1)
            renderNameESP(plr)
        end)
    end
end)

Players.PlayerRemoving:Connect(removeNameESP)

--=====================================================================
-- GET TARGETS (TEAM CHECK + IGNORE PLAYER)
--=====================================================================

local function IsSameTeam(plr1, plr2)
    return plr1.Team and plr2.Team and plr1.Team == plr2.Team
end

local function GetClosestTarget()
    local Closest, MinDist = nil, math.huge
    local MousePos = Vector2.new(Mouse.X, Mouse.Y)
    for _, plr in ipairs(Players:GetPlayers()) do
        if plr ~= Client and plr.Character then
            local Root = plr.Character:FindFirstChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('jHxAibNZrqlizOoQCpptOoRxaarmPDgwlCZhJjnJgrExKAboFSKXlFpSHVtYW5vaWRSb290UGFydA=='))
            local Humanoid = plr.Character:FindFirstChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('HgFINdQelUTtUDnyRtRbIsjWRJXSJHYfxMHvXncrjKAwNGWVrZoWuPGSHVtYW5vaWQ='))
            local BodyEffects = plr.Character:FindFirstChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('bOPvUnvPKTwuaxLRitcdhIiewnukLSpiajvvkTtKlTscYonqiUETbmdQm9keUVmZmVjdHM='))
            local KO = BodyEffects and BodyEffects:FindFirstChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('qFpESfKwdiCnXUXngmJcJRHKPwObIKHbXXoHYuiVetJDQPoUqduccuQSy5P'))
            local Grabbed = plr.Character:FindFirstChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('NkcYhQphFVYFQqATRlVrtZyKVVABLrfTzznLfGzDkqbvJdxYeIoWopYR1JBQkJJTkdfQ09OU1RSQUlOVA=='))

            if Root and Humanoid and Humanoid.Health > 0 and BodyEffects and KO and not KO.Value and not Grabbed then
                if TeamCheckEnabled and IsSameTeam(LocalPlayer, plr) then continue end
                if IgnoredPlayer and plr == IgnoredPlayer then continue end

                local screenPos, onScreen = CurrentCamera:WorldToViewportPoint(Root.Position)
                if onScreen then
                    local Dist = (Vector2.new(screenPos.X, screenPos.Y) - MousePos).Magnitude
                    if Dist < MinDist then
                        MinDist = Dist
                        Closest = plr
                    end
                end
            end
        end
    end
    return Closest
end

local function GetAllTargets()
    local targets = {}
    for _, plr in ipairs(Players:GetPlayers()) do
        if plr ~= Client and plr.Character then
            local Root = plr.Character:FindFirstChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('BLaIZrtZmEPvGrFvISsnqdiBQASJjSGLQWmuUHlQLEueHpMEyXlNpeRSHVtYW5vaWRSb290UGFydA=='))
            local Humanoid = plr.Character:FindFirstChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('JIeDnSxyhTVvgLKBYeyGoeqFSgYTsmOeSMSwVWFxjguLlQrejODKdofSHVtYW5vaWQ='))
            local BodyEffects = plr.Character:FindFirstChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('MRqEuLlGZZWEipUXUnONEozBTLjDQdHPFWAcbamOxqJOotovWWyohqwQm9keUVmZmVjdHM='))
            local KO = BodyEffects and BodyEffects:FindFirstChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('WHueUbsSsDBIaekFdkCrzifdjoExBoRaZtPjdbvFTqmELFSNRRyQtUpSy5P'))
            local Grabbed = plr.Character:FindFirstChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('RGRLSgILNKAezJUhGzHmVpZCQKPnpTQkHvOZvYdVptOZcXYkaVRSfWAR1JBQkJJTkdfQ09OU1RSQUlOVA=='))

            if Root and Humanoid and Humanoid.Health > 0 and BodyEffects and KO and not KO.Value and not Grabbed then
                if TeamCheckEnabled and IsSameTeam(LocalPlayer, plr) then continue end
                if IgnoredPlayer and plr == IgnoredPlayer then continue end

                table.insert(targets, plr)
            end
        end
    end
    return targets
end

--=====================================================================
-- BULLET TELEPORT HOOK
--=====================================================================

local mt = getrawmetatable(game)
local oldIndex = mt.__index
setreadonly(mt, false)

local lastHitOverride = nil
local lastTargetOverride = nil

mt.__index = newcclosure(function(self, key)
    if not checkcaller() and self == Mouse then
        if TargetAllEnabled then
            local targets = GetAllTargets()
            if #targets > 0 then
                local part = targets[1].Character:FindFirstChild(TargetPart)
                if part then
                    local predicted = part.Position + (part.Velocity * LeadStrength)
                    if key == RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('aNkFouAqYNpDQscalbRyXZdohAJucjDXWRkkCLcPdSDotKiZvLrHwHbSGl0') then
                        lastHitOverride = CFrame.new(predicted)
                        return lastHitOverride
                    elseif key == RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('zGyUACqKLcNJRkDLeMnBZJYyJggHKyVGQLbRSoDOwFrSFmTiNnTCpeKVGFyZ2V0') then
                        lastTargetOverride = part
                        return lastTargetOverride
                    end
                end
            end
        elseif TargetAimEnabled and Target and Target.Character then
            local part = Target.Character:FindFirstChild(TargetPart)
            if part then
                local predicted = part.Position + (part.Velocity * LeadStrength)
                if key == RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('yrolPtQeJPNSVhEFmvjOROrxhoozBOVxgHxwxzZIgIkgGSjZEcDFkZHSGl0') then
                    lastHitOverride = CFrame.new(predicted)
                    return lastHitOverride
                elseif key == RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('FgUcLhQLzYmIsRnwauYArXkBBSOxgPeevtuSkYPbOllldEITyazXDIAVGFyZ2V0') then
                    lastTargetOverride = part
                    return lastTargetOverride
                end
            end
        end
    end

    if self == Mouse and key == RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('zbgUrHoEDvLGUtpyAxYLHGyANziYCnBlNfJyshaiIuNZpKzSGbsiIjhSGl0') and lastHitOverride then
        local r = lastHitOverride; lastHitOverride = nil; return r
    elseif self == Mouse and key == RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('mPvwwqagKfvrBXderxOpJJaMFQSUfnDPDvDBBFmbYWJGPGDwwPNFgfbVGFyZ2V0') and lastTargetOverride then
        local r = lastTargetOverride; lastTargetOverride = nil; return r
    end

    return oldIndex(self, key)
end)

setreadonly(mt, true)

--=====================================================================
-- REAL TARGET VIEW
--=====================================================================

local function startViewing(plr)
    if not plr or not plr.Character then return end
    if viewDied then viewDied:Disconnect() end
    if viewChanged then viewChanged:Disconnect() end

    viewing = plr
    CurrentCamera.CameraSubject = viewing.Character

    viewDied = viewing.CharacterAdded:Connect(function()
        repeat task.wait() until viewing.Character and viewing.Character:FindFirstChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('DxcoRvmqRftuiobTycARPjzQNocaJSvafLOeWsATJrgEtpeoiFvsXjGSHVtYW5vaWRSb290UGFydA=='))
        CurrentCamera.CameraSubject = viewing.Character
    end)

    viewChanged = CurrentCamera:GetPropertyChangedSignal(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('ndyaNSQfTFbkWGnARhvAmSngSUuWRNBbEnPHdwovhKFoeJFgNWhcoDBQ2FtZXJhU3ViamVjdA==')):Connect(function()
        if viewing and CurrentCamera.CameraSubject ~= viewing.Character then
            CurrentCamera.CameraSubject = viewing.Character
        end
    end)
end

local function stopViewing()
    if viewDied then viewDied:Disconnect() end
    if viewChanged then viewChanged:Disconnect() end
    viewing = nil
    CurrentCamera.CameraSubject = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('TuCSnxhValypShQBbirmXNWleEexbClQyNeczaPGHfDVmzKidkCZWiGSHVtYW5vaWQ='))
end

--=====================================================================
-- TRACER + Q KEY
--=====================================================================

RunService.Heartbeat:Connect(function()
    if TargetAimEnabled and Target and Target.Character and not TargetAllEnabled then
        local part = Target.Character:FindFirstChild(TargetPart)
        if part then
            local predicted = part.Position + (part.Velocity * LeadStrength)
            local vec, onScreen = CurrentCamera:WorldToViewportPoint(predicted)
            if onScreen then
                Line.From = Vector2.new(Mouse.X, Mouse.Y + Inset)
                Line.To   = Vector2.new(vec.X, vec.Y)
                Line.Visible = true
            else
                Line.Visible = false
            end
        else
            Line.Visible = false
        end
    else
        Line.Visible = false
    end
end)

UserInputService.InputBegan:Connect(function(input, gp)
    if gp or input.UserInputType ~= Enum.UserInputType.Keyboard then return end
    if input.KeyCode == Enum.KeyCode.Q and TargetAimEnabled and not TargetAllEnabled then
        if Target then
            Target = nil
            if TargetViewEnabled then stopViewing() end
        else
            Target = GetClosestTarget()
            if Target and TargetViewEnabled then startViewing(Target) end
        end
    end
end)

--=====================================================================
-- GUN FINDER
--=====================================================================

local function findTool()
    for name,_ in pairs(GUN_NAMES) do
        local tool = LocalPlayer.Backpack:FindFirstChild(name) or (LocalPlayer.Character and LocalPlayer.Character:FindFirstChild(name))
        if tool then return tool end
    end
    return nil
end

--=====================================================================
-- SHOOT FUNCTION
--=====================================================================

local function shoot(targetPlayer)
    local char = LocalPlayer.Character
    if not char or char.Humanoid.Health <= 0 then return end

    local tool = currentTool or (targetAllConn and LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('mAioqrqbhAebNhvKwLufcjeqRblPGiVYMtOZtnAuJsycaGsqmTJXnQZVG9vbA==')))
    if not tool or tool.Parent ~= char then return end

    local Handler = ReplicatedStorage:WaitForChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('MuPHWFkicBPMZTMvetLgonROyCiuNTotaztrAzCNKBBxahMLBqjjqLfTWFpbk1vZHVsZQ=='))
    local Module = require(Handler)
    Module.Ignored = {workspace:WaitForChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('veMxZhknYSmRlspyjHWhtgTqLZyAamqpNIXAWOXwbjAPgZvRmcljuopVmVoaWNsZXM=')), workspace:WaitForChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('pVJLHwtVIlDmgsbNfiamqVUaSADpeCmkGyAIcGxXgKGVvFzYpwBsfXcTUFQ')), workspace:WaitForChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('sobgTRNNhBDNqDdpCTCYMlygjbMlvFcVUgIfaKpFZIizvkTNUXuDKTcSWdub3JlZA=='))}

    local handle = tool:FindFirstChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('duQuXYtTNseodshEzwPfecAzKpdDgXPOlXJnlMPXhvrmCvwkjtwvmttSGFuZGxl'))
    if not handle then return end

    local gunHandler = require(ReplicatedStorage:WaitForChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('HzEjZypHKMKfvxFiQqOlnonbRMVaFGeBVgSepKGnOXdxaphtNGDjqCbTW9kdWxlcw==')):WaitForChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('VjvtAmETVEyFBEkTMEuldpKnibbAYOMQsBHNGYhZvDLCEMbYJDLbDoFR3VuSGFuZGxlcg==')))
    local mainRemote = ReplicatedStorage:WaitForChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('qsHROpYzjpEBkVwByuopOaQDZUSvmCPYEsSuluIcjAJExOQSIQAKQEQTWFpblJlbW90ZXM=')):WaitForChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('YEjtVEyXfnLEWNOPgdDOSHoGGfIJhHavQYPFNVPVjcGOQTLJbokDHttTWFpblJlbW90ZUV2ZW50'))
    local remoteEvent = tool:FindFirstChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('pNzECgRqxNyxpSAncrjVqXCPPdABJgLYsaMEJDgqzvTjwbutDwMQtBKUmVtb3RlRXZlbnQ=')) or {FireServer = function() end}

    if not gunHandler.getCanShoot(char) then return end

    local offset = CFrame.new(-1, 0.4, 0)
    local muzzlePos = (handle.CFrame * offset).Position

    local aimPos
    if targetPlayer and targetPlayer.Character then
        local part = targetPlayer.Character:FindFirstChild(TargetPart)
        if part then aimPos = part.Position + (part.Velocity * LeadStrength) end
    end

    local infRange = 999999
    local success, data, hit, pos = pcall(function()
        return gunHandler.shoot({
            Shooter      = char,
            Handle       = handle,
            ForcedOrigin = muzzlePos,
            AimPosition  = aimPos or (muzzlePos + gunHandler.getAim(muzzlePos) * infRange),
            BeamColor    = Color3.fromRGB(255,140,38),
            Range        = infRange
        })
    end)

    if not success or not data then return end

    pcall(function()
        mainRemote:FireServer(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('vxYXikXIwGztMMErXNkmKQIOMYszUoWYcuTrDgvnZAjKYYVupjcShbjc2hvdGFndW56ZWVoTzBkeEQ='), handle, muzzlePos, data, hit or false, pos or Vector3.zero)
    end)

    pcall(function()
        if remoteEvent.FireServer then remoteEvent:FireServer(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('zubeWHKLTrDxMSWvKnwExltVTRqpkRyiVNWMTxQqXjAlDUaPFthwsLLU2hvb3Q=')) end
    end)
end

--=====================================================================
-- RAPID FIRE
--=====================================================================

local function disconnectRapidFire()
    if heartbeatConn and heartbeatConn.Connected then heartbeatConn:Disconnect() heartbeatConn = nil end
    for _, c in ipairs(connections) do if c and c.Connected then c:Disconnect() end end
    connections = {}
end

local function enableRapidFire()
    if revolverEnabled then return end
    revolverEnabled = true
    holding = false
    disconnectRapidFire()

    currentTool = findTool()
    if not currentTool then return end

    local handle = currentTool:WaitForChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('WwqPoIKZhblgRayonHcfpURrCynSkOWBOsQaPLEVIQSOQEIdAlRpewlSGFuZGxl'),5)
    local ammo   = currentTool:WaitForChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('yvHevVmIuaUVZAnkxmTAJKlLpMxsIylvXTLHrQwuSxSnmLmCxSXUmXCQW1tbw=='),5)
    local cd     = currentTool:WaitForChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('dDzOCIucwwwFEXSbtwmULKiBjzoozCyexoLgpRaBbufcBGQHireZHxFU2hvb3RpbmdDb29sZG93bg=='),5)
    if not (handle and ammo and cd) then return end

    cd.Value = 0.01
    local maxAmmo = ammo.Value

    table.insert(connections, ammo:GetPropertyChangedSignal(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('HZqIRTsqJXWkmqShAvEkwppwxPfqEDONrTQzFBZycEoGdgPuLUgZnJpVmFsdWU=')):Connect(function()
        if revolverEnabled then ammo.Value = maxAmmo end
    end))
    table.insert(connections, RunService.Heartbeat:Connect(function()
        if revolverEnabled then ammo.Value = maxAmmo end
    end))

    table.insert(connections, UserInputService.InputBegan:Connect(function(i,p)
        if p or i.UserInputType ~= Enum.UserInputType.MouseButton1 then return end
        if not (revolverEnabled and currentTool and currentTool.Parent == LocalPlayer.Character) then return end

        holding = true
        if heartbeatConn and heartbeatConn.Connected then heartbeatConn:Disconnect() end
        heartbeatConn = RunService.Heartbeat:Connect(function()
            if not (revolverEnabled and holding and currentTool and currentTool.Parent == LocalPlayer.Character) then
                holding = false
                if heartbeatConn and heartbeatConn.Connected then heartbeatConn:Disconnect() heartbeatConn = nil end
                return
            end
            shoot(nil)
        end)
    end))

    table.insert(connections, UserInputService.InputEnded:Connect(function(i)
        if i.UserInputType == Enum.UserInputType.MouseButton1 then holding = false end
    end))

    local activatedConn = currentTool.Activated:Connect(function() shoot(nil) end)
    table.insert(connections, activatedConn)

    table.insert(connections, currentTool:GetPropertyChangedSignal(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('ATnpBJuVbgMEHvLBraWIswkuAQVKafpOnUISEGkqgbTLqzqnDYNceNER3JpcA==')):Connect(function()
        ReplicatedStorage:WaitForChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('fBIrxEMerJaqyeMfjoBLZxgLFfWmepqmvbxHUNQCWMtJLLDDtHVTGDjTWFpblJlbW90ZXM=')):WaitForChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('rIhLumicHwvkILTbqiWCDjuQVEoFwRVKqTQNfeFpePoQfYEgsgQIAfCTWFpblJlbW90ZUV2ZW50')):FireServer(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('LwbBLxDqUyREsOkGtAgFNYgQobEilUKMEuNgJdwDEidvvyYVYgEQYgrQ0hFQ0tFUl80'))
    end))
end

local function disableRapidFire()
    revolverEnabled = false
    holding = false
    currentTool = nil
    disconnectRapidFire()
    Library:Notification(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('lHBiTyBEjChcBiNHgBVwSjYWijmCoGZPOjYLOQXCgeAlhbAGXsWQtAWUmFwaWQgRmlyZSBESVNBQkxFRA=='), 2, Color3.fromRGB(255,100,100))
end

--=====================================================================
-- TARGET ALL
--=====================================================================

local function disconnectTargetAll()
    if targetAllConn and targetAllConn.Connected then targetAllConn:Disconnect() targetAllConn = nil end
    for _, c in ipairs(targetAllConnections) do if c and c.Connected then c:Disconnect() end end
    targetAllConnections = {}
end

local function startTargetAllRapidFire()
    if targetAllConn and targetAllConn.Connected then return end

    local tool = findTool()
    if not tool then return end

    local handle = tool:WaitForChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('msXlfzemWIFkXgRcjmkupOTKIcmMbggriaPBOlcntaicLlFbkXAqIgRSGFuZGxl'),5)
    local ammo   = tool:WaitForChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('xTtYDnqKroSrzaTautYKWpBUubWqjwpPUczYWJoPdRsdRbbhvGCPqVtQW1tbw=='),5)
    local cd     = tool:WaitForChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('UEfEdGAWyAMsDaOCJKSvgGcaZQSMRcnKiEFvJKsXGpiXBojsbLQtqJmU2hvb3RpbmdDb29sZG93bg=='),5)
    if not (handle and ammo and cd) then return end

    cd.Value = 0.01
    local maxAmmo = ammo.Value

    table.insert(targetAllConnections, ammo:GetPropertyChangedSignal(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('ECzKkLyilGhMIzxGpNDQGRCSdRadqewecpbvvYimFZGlLpyrIFpJmTGVmFsdWU=')):Connect(function()
        if TargetAllEnabled then ammo.Value = maxAmmo end
    end))
    table.insert(targetAllConnections, RunService.Heartbeat:Connect(function()
        if TargetAllEnabled then ammo.Value = maxAmmo end
    end))

    targetAllConn = RunService.Heartbeat:Connect(function()
        if not (TargetAllEnabled and tool and tool.Parent == LocalPlayer.Character) then return end
        local targets = GetAllTargets()
        for _, t in ipairs(targets) do
            shoot(t)
            task.wait(0.01)
        end
    end)
end

local function stopTargetAllRapidFire()
    TargetAllEnabled = false
    disconnectTargetAll()
    Library:Notification(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('kWJothrUqLMeHZCyzGnCoTGSwVGZsjmWMoNbFKPjEJHfxIukQkmBnXXVGFyZ2V0IEFsbCBESVNBQkxFRA=='), 2, Color3.fromRGB(255,100,100))
end

--=====================================================================
-- FOV CHANGER
--=====================================================================

local function updateFOV()
    if FOVEnabled and CurrentCamera then
        CurrentCamera.FieldOfView = TargetFOV
    end
end

local function enableFOVChanger()
    if FOVConnection then FOVConnection:Disconnect() end
    FOVConnection = RunService.RenderStepped:Connect(updateFOV)
end

local function disableFOVChanger()
    if FOVConnection then FOVConnection:Disconnect() FOVConnection = nil end
    if CurrentCamera then CurrentCamera.FieldOfView = 80 end
end

--=====================================================================
-- FLY LOGIC
--=====================================================================

local function startFlying()
    if isFlying then return end
    local char = LocalPlayer.Character
    local root = char and char:FindFirstChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('HWeItbEwEXlzxghhpGfGOCqvJvaUdrUiRjpgIGUYmCmKlBEknoHyzQeSHVtYW5vaWRSb290UGFydA=='))
    local hum = char and char:FindFirstChildWhichIsA(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('cFHEUfIjayuHGMyGwwOsQLsHCUfXQECqtdvTGLsFHymbkFvXKkqTEqQSHVtYW5vaWQ='))
    if not root or not hum then return end

    isFlying = true
    local oldState = hum:GetState()
    root:SetAttribute(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('mBLYzPFsUrEnxKFiYKBztxuPVvduJQAzcayRRjpiNBBtuFeCjWzPGDlT2xkSHVtYW5vaWRTdGF0ZQ=='), oldState)
    hum:ChangeState(Enum.HumanoidStateType.Physics)

    flyConnection = RunService.RenderStepped:Connect(function(delta)
        if not (flyKeybindActive and isFlying and root and root.Parent) then return end
        local cam = CurrentCamera
        local moveDir = Vector3.zero

        if UserInputService:IsKeyDown(Enum.KeyCode.W) then moveDir += cam.CFrame.LookVector end
        if UserInputService:IsKeyDown(Enum.KeyCode.S) then moveDir -= cam.CFrame.LookVector end
        if UserInputService:IsKeyDown(Enum.KeyCode.A) then moveDir -= cam.CFrame.RightVector end
        if UserInputService:IsKeyDown(Enum.KeyCode.D) then moveDir += cam.CFrame.RightVector end

        if moveDir.Magnitude > 0 then
            moveDir = moveDir.Unit * (flySpeed * delta * 60)
            root.CFrame += moveDir
        end

        local targetRot = CFrame.lookAt(root.Position, root.Position + cam.CFrame.LookVector)
        root.CFrame = root.CFrame:Lerp(targetRot, 0.2)
        root.AssemblyLinearVelocity = Vector3.zero
        root.AssemblyAngularVelocity = Vector3.zero
    end)

    Library:Notification(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('FiAbdxUgdeGgFSXNjkMTMkNxwhoqWLKIFxgbzSXysXGFBaKhgWltupZRmx5IE9OIChGKQ=='), 2, Color3.fromRGB(0, 255, 255))
end

local function stopFlying()
    if not isFlying then return end
    isFlying = false
    if flyConnection then flyConnection:Disconnect() flyConnection = nil end

    local char = LocalPlayer.Character
    local root = char and char:FindFirstChild(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('qkiodphgTWbwTcwDSQmRypRMysDoFhzkIUFZgcFpqYFmadDjNghjmXbSHVtYW5vaWRSb290UGFydA=='))
    local hum = char and char:FindFirstChildWhichIsA(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('fWcgNbhpTMfpkGVikfSFivkgaIxNVJurPVJSHPjDaUdGTUZzxCjrLyuSHVtYW5vaWQ='))
    if hum and root then
        local oldState = root:GetAttribute(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('pxZHKdyymWqhwJiqrlXvvpIqpLZYSQxZFBkxVBZDoBzTzEdFeJJSXLcT2xkSHVtYW5vaWRTdGF0ZQ=='))
        hum:ChangeState(oldState or Enum.HumanoidStateType.GettingUp)
    end

    Library:Notification(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('FSxZdDEORfVsxIazgoKyWvxCFfcCaSWeKmpDitZMCHryluyinpRNroVRmx5IE9GRiAoRik='), 2, Color3.fromRGB(255, 100, 100))
end

--=====================================================================
-- TP WALK LOGIC
--=====================================================================

task.spawn(function()
    while task.wait() do
        if tpKeybindActive and LocalPlayer.Character then
            local chr = LocalPlayer.Character
            local hum = chr:FindFirstChildWhichIsA(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('XPrpvkPodbjUGMyQfvYlvhommCsjcUotHpstxCMKMlkujIrBZtscIDKSHVtYW5vaWQ='))
            if chr and hum and hum.MoveDirection.Magnitude > 0 then
                chr:TranslateBy(hum.MoveDirection * tpSpeed * 10 * RunService.Heartbeat:Wait())
            end
        end
    end
end)

