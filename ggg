--=====================================================================
-- KEYBINDS
--=====================================================================

UserInputService.InputBegan:Connect(function(input, gp)
    if gp then return end
    if input.KeyCode == Enum.KeyCode.Z then
        if tpKeybindEnabled then
            tpKeybindActive = not tpKeybindActive
            Library:Notification(tpKeybindActive and RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('cHmeaUtQkCmnHrVycyDjWqpFlzqninffJVGjsjhXrSEJauIffnROEOTVFAgV2FsayBPTiAoWik=') or RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('smnKNNImSRVvIhdVrGczjNTXLfdyUKfnamEdEmwUbVjQUTxkRCqtZdqVFAgV2FsayBPRkYgKFop'), 2, tpKeybindActive and Color3.fromRGB(0,255,0) or Color3.fromRGB(255,100,100))
        end
    elseif input.KeyCode == Enum.KeyCode.F then
        if flyKeybindEnabled then
            flyKeybindActive = not flyKeybindActive
            if flyKeybindActive then
                startFlying()
            else
                stopFlying()
            end
        else
            Library:Notification(RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('xWKpTczJzWbzRgOtgawvzaAXACpIXShRgwoeimRvfOKCiMcWDHjoIZZRW5hYmxlIEZseSBpbiBVSSBmaXJzdCE='), 2, Color3.fromRGB(255,150,0))
        end
    end
end)

LocalPlayer.CharacterAdded:Connect(function()
    task.wait(0.5)
    if flyKeybindActive then startFlying() end
end)

--=====================================================================
-- AUTO-EQUIP
--=====================================================================

LocalPlayer.CharacterAdded:Connect(function()
    task.wait(1)
    if Library.Flags[RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('rbspcQxVAqebEVfgTUHZrusYbIKsNodwXWrxzgGjdXvtYFejTdYsAfYUmFwaWRGaXJl')] then enableRapidFire() end
    if Library.Flags[RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('GwexDBoImINvnhtjSZhFCXhwZUMlfLlzKmLPPxgjSGbwmERjGzvhUldVGFyZ2V0QWxs')] then startTargetAllRapidFire() end
end)

LocalPlayer.Backpack.ChildAdded:Connect(function(obj)
    if GUN_NAMES[obj.Name] then
        task.spawn(function()
            task.wait(0.2)
            if Library.Flags[RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('JhjlklNmupJCPOlDLdADRPTwhQBXIeQNIsDcBgUYULMqQRKhnFXzOFWUmFwaWRGaXJl')] then enableRapidFire() end
            if Library.Flags[RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('dBFTfPzKiePLKuIbGYnRLjvSSpvRyddgQtLcDBsgQHoSUjvDYYTmWGeVGFyZ2V0QWxs')] then startTargetAllRapidFire() end
        end)
    end
end)

task.spawn(function()
    task.wait(2)
    if LocalPlayer.Character then
        for _, c in ipairs(LocalPlayer.Character:GetChildren()) do
            if GUN_NAMES[c.Name] then
                if Library.Flags[RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('tGvRsDnUWtfdUlpQvOSIiDDhmmfosbYQZiNZEBqXheXsxQkJwzoNUFbUmFwaWRGaXJl')] then enableRapidFire() end
                if Library.Flags[RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('ELMoEjfEcbgjZJHXmUrRmHNVdJysRAQInpnaaEUAeHDIivLNWLpTbPtVGFyZ2V0QWxs')] then startTargetAllRapidFire() end
                break
            end
        end
    end
end)

