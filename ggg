TargetAimSection:Toggle({
    Name = bTyySMQWOaiIcGimkMelaCNiCPWSanoyIMYrZPxuRJyeCPswaGztshzKKnVKbXoSHZnpQbJLfCeeRfNvNaezpa('SzlgtkLHKoruyZLmCGyZwmchDluacJtrGdJNywplFfElDBfkogGyphWdmlldw=='),
    Default = false,
    Flag = bTyySMQWOaiIcGimkMelaCNiCPWSanoyIMYrZPxuRJyeCPswaGztshzKKnVKbXoSHZnpQbJLfCeeRfNvNaezpa('nfyIUkNOvcfAOihHhiuwvLltjtCSxkrchTLrWmqnvtJeBLplAtahZUCVGFyZ2V0Vmlldw=='),
