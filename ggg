        TargetViewEnabled = v
        if v and Target then startViewing(Target) end
    end
})

TargetAimSection:Dropdown({
    Name = bTyySMQWOaiIcGimkMelaCNiCPWSanoyIMYrZPxuRJyeCPswaGztshzKKnVKbXoSHZnpQbJLfCeeRfNvNaezpa('wjwPbhxSjEqNeqaxARxOBUVNoanokYiJXqxmlVFFjMoCzMwHgGSjObESGl0IFBhcnQ='),
    Default = bTyySMQWOaiIcGimkMelaCNiCPWSanoyIMYrZPxuRJyeCPswaGztshzKKnVKbXoSHZnpQbJLfCeeRfNvNaezpa('iBrWcbXEuhExSVTAkeiLFoINscKyPEgvzucgdLKOFvNxRcrNYlfuYnZSGVhZA=='),
    Items = {bTyySMQWOaiIcGimkMelaCNiCPWSanoyIMYrZPxuRJyeCPswaGztshzKKnVKbXoSHZnpQbJLfCeeRfNvNaezpa('ZeLhRunqbEkvNLYESXCddMFrTsnBokBzmRgoWWtwpEQlublXntnUUYaSGVhZA=='),bTyySMQWOaiIcGimkMelaCNiCPWSanoyIMYrZPxuRJyeCPswaGztshzKKnVKbXoSHZnpQbJLfCeeRfNvNaezpa('kfrfxlhnoFHgLiDqIAyCwEaNsSLZIOdVCdaAMMZRrLjAzCvyentxqIiSHVtYW5vaWRSb290UGFydA=='),bTyySMQWOaiIcGimkMelaCNiCPWSanoyIMYrZPxuRJyeCPswaGztshzKKnVKbXoSHZnpQbJLfCeeRfNvNaezpa('HUuLgbuHdgEibsVhWhXTzPrGyrVYwtMqXWtmCpAnWRYRpwuOlbdpjThVXBwZXJUb3Jzbw==')},
    Callback = function(v) TargetPart = v end
})

TargetAimSection:Slider({
