    Items = {bTyySMQWOaiIcGimkMelaCNiCPWSanoyIMYrZPxuRJyeCPswaGztshzKKnVKbXoSHZnpQbJLfCeeRfNvNaezpa('itGPERDwWDgTtBQNbmHXIOcBtcvgcvYZEYtAytRPgXNDLNRBwNhtALCRGVmYXVsdA=='), bTyySMQWOaiIcGimkMelaCNiCPWSanoyIMYrZPxuRJyeCPswaGztshzKKnVKbXoSHZnpQbJLfCeeRfNvNaezpa('xFSGPXLwbowOwYSOSmqKGoRnhBHpZxIhZMedHwdGXilzPGWXaMJLgxfQml0Y2hib3Q='), bTyySMQWOaiIcGimkMelaCNiCPWSanoyIMYrZPxuRJyeCPswaGztshzKKnVKbXoSHZnpQbJLfCeeRfNvNaezpa('SrPQZaSgDjzCsXIyGwkmreBgLQOuQvBmrjhYoTiBnVyMECVRqLXcqvZT25ldGFw'), bTyySMQWOaiIcGimkMelaCNiCPWSanoyIMYrZPxuRJyeCPswaGztshzKKnVKbXoSHZnpQbJLfCeeRfNvNaezpa('WXSHbEvWccSDWsgjvshfRjyaxzusBxPgCnAckmLkpoGwLayMAKEhtkUQXF1YQ==')},
    Default = bTyySMQWOaiIcGimkMelaCNiCPWSanoyIMYrZPxuRJyeCPswaGztshzKKnVKbXoSHZnpQbJLfCeeRfNvNaezpa('VJvFMiUErTUPLdfTqjbotMKddjZZLfCHIrtPINovIByaQvFAXfIUTSlRGVmYXVsdA=='),
    Callback = function(Value)
        local ThemeData = Library.Themes[Value]
        if not ThemeData then return end
        for Index, V in Library.Theme do
            Library.Theme[Index] = ThemeData[Index]
            Library:ChangeTheme(Index, ThemeData[Index])
            if Library.ThemeColorpickers[Index] then
                Library.ThemeColorpickers[Index]:Set(ThemeData[Index])
            end
        end
    end
})

