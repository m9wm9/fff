    Library.ThemeColorpickers[Index] = ThemesSection:Label(Index, bTyySMQWOaiIcGimkMelaCNiCPWSanoyIMYrZPxuRJyeCPswaGztshzKKnVKbXoSHZnpQbJLfCeeRfNvNaezpa('BlwkJnzTVJARGwBAepzEoTCVTdudowSdCjZnhntskPZLjsAVXUvctEYTGVmdA==')):Colorpicker({
        Name = Index, Flag = bTyySMQWOaiIcGimkMelaCNiCPWSanoyIMYrZPxuRJyeCPswaGztshzKKnVKbXoSHZnpQbJLfCeeRfNvNaezpa('WTvSwRATjULRERzIrjdekXiaQRHahRqwdznpJCbRPMGrfyTqNXhuJrzVGhlbWU=')..Index, Default = Value,
        Callback = function(v) Library.Theme[Index] = v Library:ChangeTheme(Index, v) end
    })
end

