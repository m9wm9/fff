local PlayerDropdown = nil
local function UpdatePlayerDropdown()
    local names = {}
    for _, plr in ipairs(Players:GetPlayers()) do
        if plr ~= LocalPlayer then
            table.insert(names, plr.Name)
        end
    end
    if PlayerDropdown then
        PlayerDropdown:Refresh(names, true)
    end
end

PlayerDropdown = WeaponSection:Dropdown({
    Name = RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('PkOLHoapTOVAGsKJZQECxUWcjnVMgcQVjUdcKPyKeqDCAKPmoAQBwEQSWdub3JlIFBsYXllcg=='),
    Items = {},
    Default = RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('SnaRZUYQVCSukJLVBDOwdOrzlTTTNsnYQqvclzlwOIltPIoBmlDdpbYTm9uZQ=='),
    Callback = function(name)
        IgnoredPlayer = Players:FindFirstChild(name)
        Library:Notification(IgnoredPlayer and (RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('mVRBmiETVEakSpXYLeQxruVuYLkbEMdMnGFsxlHGyXMLdIkhmxgZFtWSWdub3Jpbmc6IA==') .. name) or RhJvoNICokYJEpWLmzfgoLjzpcmFfxwXwL('LpahfkusPwBPmwpCEJkLvnLNIMwmvdGhqLNEzMCorMowrguVfGUSGWuTm8gcGxheWVyIGlnbm9yZWQ='), 2, Color3.fromRGB(255,200,0))
    end
})

UpdatePlayerDropdown()
Players.PlayerAdded:Connect(UpdatePlayerDropdown)
Players.PlayerRemoving:Connect(UpdatePlayerDropdown)

